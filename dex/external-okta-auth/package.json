    "jasmine": "^3.0.0",
    "jasmine-reporters": "2.2.0",
    "platform": "^1.3.5",
    "protractor": "^5.4.2",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "@okta/jwt-verifier": "1.0.0",
    "@okta/oidc-middleware": "^3.0.0",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "mustache-express": "^1.2.5"
  }
